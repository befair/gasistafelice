{% extends "blocks/stocks/table.xml" %}
{% load i18n %}

{% block headers %}
    <th>{% trans "Id" %}</th>
    <th title='{% trans "Order" %}'>Ord.</th>
    <th>{% trans "Supplier" %}</th>
    <th>{% trans "Product" %}</th>
    <th>{% trans "Price" %}</th>
    <th title='{% trans "Price changed" %}'>&euro;&lt;&gt;</th>
    <th title='{% trans "Ordered amount" %}'>{% trans "Amount" %}</th>
    <th title='{% trans "Ordered total" %}'>{% trans "Total" %}</th>
    <th title='{% trans "Delete" %}'>Del</th>
    <th title='Ordine confermato?'>Ok</th>
{% endblock %}

{% block footer %}
    <tr>
        <th class="totfield" colspan="5">{% trans "Total" %}:</th>
        <th class="totvalue" colspan="5" id="total-order"></th>
    </tr>
{% endblock %}

